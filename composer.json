{
    "name": "viralsbackpack/backpackimageupload",
    "description": "Manager images in laravel backpack",
    "license": "license",
    "authors": [
        {
            "name": "virals manhhd",
            "email": "manhhd@viralsoft.vn",
            "homepage": "https://github.com/viralsoft"
        }
    ],
    "homepage": "https://github.com/viralsbackpack/backpackimageupload",
    "keywords": ["Laravel", "BackPackImageUpload"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ViralsBackpack\\BackPackImageUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ViralsBackpack\\BackPackImageUpload\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViralsBackpack\\BackPackImageUpload\\BackPackImageUploadServiceProvider"
            ],
            "aliases": {
                "BackPackImageUpload": "ViralsBackpack\\BackPackImageUpload\\Facades\\BackPackImageUpload"
            }
        }
    }
}
